var i = 0

while i <= 100 do
  when 
    i / 3 == 0 => "Fizz",
    i / 5 == 0 => "Buzz",
    i / 3 == 0 and i / 3 == 0 => "FizzBuzz",
    else => "Ignore"
  end
  i = i + 1
end







