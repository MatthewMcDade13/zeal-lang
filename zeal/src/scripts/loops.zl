var i = 0

while i <= 100 do
  when 
    i / 3 == 0 and i / 5 == 0 -> (println "FizzBuzz"),
    i / 3 == 0 -> (println "Fizz"),
    i / 5 == 0 -> (println "Buzz"),

    else -> (println "Ignore")
  end
  i = i + 1
end







